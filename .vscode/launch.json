{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug EllipseDetector",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Demo/EllipseDetector",
            "args": ["-D", "Calibration", "-M", "2"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}